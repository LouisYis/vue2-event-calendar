{
  "name": "vue2-event-calendar",
  "version": "0.1.0",
  "description": "A vue2 calendar component, have week and month two mode, custom render event",
  "main": "dist/vue2-event-calendar.js",
  "scripts": {
    "dev": "parcel index.html -d demo",
    "build": "parcel build index.html -d demo",
    "deploy": "yarn test && yarn build && parcel build src/index.js -o vue2-event-calendar.min.js && parcel build src/index.js -o vue2-event-calendar.js --no-minify",
    "lint": "eslint src test",
    "unit": "jest",
    "test": "npm run lint && npm run unit"
  },
  "keywords": [
    "vue",
    "calendar"
  ],
  "author": "kitwang chan",
  "license": "MIT",
  "repository": "https://github.com/kitwon/vue2-calendar",
  "devDependencies": {
    "@vue/test-utils": "^1.0.0-beta.12",
    "babel-eslint": "^8.2.2",
    "babel-jest": "^22.4.1",
    "babel-preset-env": "^1.6.1",
    "babel-preset-stage-2": "^6.24.1",
    "eslint": "^4.18.2",
    "eslint-config-standard": "^11.0.0",
    "eslint-plugin-html": "^4.0.2",
    "eslint-plugin-import": "^2.9.0",
    "eslint-plugin-node": "^6.0.1",
    "eslint-plugin-promise": "^3.6.0",
    "eslint-plugin-standard": "^3.0.1",
    "jest": "^22.4.2",
    "jest-serializer-vue": "^0.3.0",
    "less": "^3.0.1",
    "parcel-bundler": "^1.6.2",
    "parcel-plugin-vue": "^1.5.0",
    "postcss-flexbugs-fixes": "^3.3.0",
    "vue-jest": "^2.1.1",
    "vue-template-compiler": "^2.5.13"
  },
  "dependencies": {
    "moment": "^2.20.1",
    "vue": "^2.5.13"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "vue"
    ],
    "transform": {
      "^.+\\.js$": "<rootDir>/node_modules/babel-jest",
      ".*\\.(vue)$": "<rootDir>/node_modules/vue-jest"
    },
    "moduleNameMapper": {
      "\\.(css|less|scss|sass)$": "<rootDir>/test/__mocks__/styleMock.js"
    },
    "snapshotSerializers": [
      "<rootDir>/node_modules/jest-serializer-vue"
    ]
  }
}
